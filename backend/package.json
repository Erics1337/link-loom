    "ioredis": "^5.8.2",
    "ml-kmeans": "^7.0.0",
    "openai": "^6.9.1",
    "pg": "^8.16.3",
    "pgvector": "^0.2.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/node": "^24.10.1",
